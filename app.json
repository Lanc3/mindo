{
  "expo": {
    "jsEngine": "jsc",
    "name": "mindo",
    "slug": "mindo",
    "version": "1.0.57",
    "orientation": "portrait",
    "icon": "./assets/mindo-app-icon-applestore.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 31,
            "buildToolsVersion": "31.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#6e822b",
      "androidMode": "default",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ie.medicalindependant.mindo"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ie.medicalindependant.mindo",
      "versionCode": 57
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
